{
  "name": "@batpb/genart",
  "version": "2.0.0-rc.3-alpha.2",
  "description": "A TypeScript compatible library built with p5.js for creating responsive, generative, algorithmic art projects.",
  "license": "MIT",
  "type": "module",
  "types": "./_dist/index.d.cts",
  "module": "./_dist/index.mjs",
  "main": "./_dist/index.cjs",
  "exports": {
    ".": {
      "import": {
        "types": "./_dist/index.d.mts",
        "default": "./_dist/index.mjs"
      },
      "require": {
        "types": "./_dist/index.d.cts",
        "default": "./_dist/index.cjs"
      }
    }
  },
  "files": [
    "./_dist",
    "./LICENSE",
    "./package.json",
    "./README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git@https://github.com/brittni-and-the-polar-bear/genart.git"
  },
  "bugs": {
    "url": "https://github.com/brittni-and-the-polar-bear/genart/issues"
  },
  "homepage": "https://brittni-and-the-polar-bear.github.io/genart/",
  "author": {
    "name": "brittni and the polar bear",
    "url": "https://brittni-and-the-polar-bear.github.io/"
  },
  "contributors": [
    {
      "name": "brittni watkins",
      "url": "https://blwatkins.github.io/"
    },
    {
      "name": "azurepolarbear",
      "url": "https://azurepolarbear.github.io/"
    }
  ],
  "keywords": [
    "algorithmic art",
    "code art",
    "genart",
    "generative art",
    "p5",
    "p5.js",
    "p5-js",
    "p5.js library",
    "typescript"
  ],
  "scripts": {
    "build": "tsdown",
    "docs": "typedoc",
    "lint:js": "eslint -c eslint.config.js.mjs ./eslint.config.js.mjs ./eslint.config.ts.mjs",
    "lint:ts": "eslint -c eslint.config.ts.mjs ./src ./tests ./tsdown.config.ts ./vitest.config.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "prepack": "npm run build"
  },
  "dependencies": {
    "@types/nearest-color": "^0.4.1",
    "@types/p5": "^1.7.7",
    "nearest-color": "^0.4.4",
    "p5": "^1.11.11"
  },
  "peerDependencies": {
    "color-name-list": "^13.41.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@stylistic/eslint-plugin": "^5.6.1",
    "@vitest/coverage-v8": "^4.0.15",
    "@vitest/ui": "^4.0.14",
    "canvas": "^3.2.0",
    "eslint": "^9.39.1",
    "eslint-plugin-es-x": "^9.2.0",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-security": "^3.0.1",
    "jsdom": "^27.2.0",
    "ts-node": "^10.9.2",
    "tsdown": "^0.17.2",
    "tslib": "^2.8.1",
    "typedoc": "^0.28.15",
    "typedoc-plugin-coverage": "^4.0.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.0",
    "vitest": "^4.0.14"
  }
}
