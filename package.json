    },
    {
      "name": "azurepolarbear",
      "url": "https://linktr.ee/azurepolarbear"
    }
  ],
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/brittni-and-the-polar-bear/generative-art-library/issues"
  },
  "homepage": "https://brittni-and-the-polar-bear.github.io/generative-art-library/",
  "dependencies": {
