    "@types/eslint__js": "^8.42.3",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "canvas": "^2.11.2",
    "eslint": "^9.8.0",
    "eslint-plugin-es-x": "^8.0.0",
    "eslint-plugin-n": "^17.10.2",
    "eslint-plugin-security": "^3.0.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "ts-jest": "^29.2.3",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.3",
