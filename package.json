      },
      "require": {
        "types": "./_dist/index.d.ts",
        "default": "./_dist/index.js"
      }
    }
  },
  "files": [
    "./_dist",
    "./LICENSE",
    "./package.json",
    "./README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git@https://github.com/brittni-and-the-polar-bear/genart.git"
  },
  "bugs": {
    "url": "https://github.com/brittni-and-the-polar-bear/genart/issues"
  },
  "homepage": "https://brittni-and-the-polar-bear.github.io/genart/",
  "author": {
    "name": "brittni and the polar bear",
    "url": "https://brittni-and-the-polar-bear.github.io/"
  },
  "contributors": [
    {
      "name": "brittni watkins",
      "url": "https://blwatkins.github.io/"
    },
    {
      "name": "azurepolarbear",
      "url": "https://azurepolarbear.github.io/"
    }
  ],
  "keywords": [
    "algorithmic art",
    "code art",
    "genart",
    "generative art",
    "p5",
    "p5.js",
    "p5-js",
    "p5.js library",
    "typescript"
