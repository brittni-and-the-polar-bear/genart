{
  "$schema": "https://developer.microsoft.com/json-schemas/api-extractor/v7/api-extractor.schema.json",
  "projectFolder": ".",
  "mainEntryPointFilePath": "<projectFolder>/out/dist/types/index.d.ts",
  "bundledPackages": [
    "p5"
  ],
  "newlineKind": "lf",
  "enumMemberOrder": "preserve",
  "compiler": {
    "tsconfigFilePath": "<projectFolder>/tsconfig.json"
  },
  "apiReport": {
    "enabled": true,
    "reportFileName": "<unscopedPackageName>.api.md",
    "reportFolder": "<projectFolder>/api/",
    "reportTempFolder": "<projectFolder>/api-temp/",
    "includeForgottenExports": false
  },
  "docModel": {
    "enabled": true,
    "apiJsonFilePath": "<projectFolder>/api/<unscopedPackageName>.api.json",
    "includeForgottenExports": false,
    "projectFolderUrl": "https://github.com/brittni-and-the-polar-bear/generative-art-library"
  },
  "dtsRollup": {
    "enabled": true,
    "untrimmedFilePath": "<projectFolder>/out/dist/index.d.ts",
    "alphaTrimmedFilePath": "<projectFolder>/out/types/<unscopedPackageName>-alpha.d.ts",
    "betaTrimmedFilePath": "<projectFolder>/out/types/<unscopedPackageName>-beta.d.ts",
    "publicTrimmedFilePath": "<projectFolder>/out/types/<unscopedPackageName>-public.d.ts"
  },
  "tsdocMetadata": {
    "enabled": true,
    "tsdocMetadataFilePath": "<projectFolder>/out/dist/tsdoc-metadata.json"
  },
  "messages": {
    "compilerMessageReporting": {
      "default": {
        "logLevel": "warning",
        "addToApiReportFile": false
      }
    },
    "extractorMessageReporting": {
      "default": {
        "logLevel": "warning",
        "addToApiReportFile": false
      },
      "ae-forgotten-export": {
        "logLevel": "warning",
        "addToApiReportFile": false
      }
    },
    "tsdocMessageReporting": {
      "default": {
        "logLevel": "warning",
        "addToApiReportFile": false
      }
    }
  }
}
