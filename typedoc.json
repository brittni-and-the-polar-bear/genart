{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": [
    "./src/main/batpb-genart.ts"
  ],
  "out": "./out/doc",

  "plugin": [
    "typedoc-plugin-extra-footer",
    "typedoc-plugin-inline-sources",
    "typedoc-plugin-coverage",
    "typedoc-plugin-mdn-links"
  ],

  "includeVersion": true,

  "json": "./out/doc/json",
  "navigationLinks": {
    "GitHub Pages": "https://brittni-and-the-polar-bear.github.io/generative-art-library/"
  },
  "sidebarLinks": {

  },
  "navigation": {
    "includeCategories": true,
    "includeGroups": true,
    "includeFolders": true
  },
  "visibilityFilters": {
    "private": true,
    "protected": true,
    "inherited": true,
    "external": true,
    "@alpha": true,
    "@beta": true,
    "@deprecated": true
  },

  "sort": ["source-order"],

  "validation": {
    "notExported": true,
    "invalidLink": true,
    "notDocumented": true
  },
  "treatWarningsAsErrors": false,

  "extraFooter": "<br/>Copyright &copy; 2023-2024 <a href=\"https://brittni-and-the-polar-bear.github.io/\" target=\"_blank\" rel=\"noopener noreferrer\">brittni and the polar bear LLC</a>",

  "externalSymbolLinkMappings": {
    "global": {
      "P5Lib": "https://p5js.org/reference/#/p5/p5",
      "P5Lib.Color": "https://p5js.org/reference/#/p5.Color"
    },
    "p5": {
      "P5Lib": "https://p5js.org/reference/#/p5/p5",
      "p5": "https://p5js.org/reference/#/p5/p5"
    },
    "@types/p5": {
      "P5Lib": "https://p5js.org/reference/#/p5/p5",
      "p5": "https://p5js.org/reference/#/p5/p5"
    }
  }
}
